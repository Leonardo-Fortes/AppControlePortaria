<Window x:Class="AppPortariaControle.Views.ColabReponsavel"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:AppPortariaControle.Views"
      mc:Ignorable="d" 
      d:DesignHeight="200" d:DesignWidth="400"
      Title="ColabReponsavel" MaxHeight="200" MaxWidth="400"
      WindowStartupLocation="CenterScreen">

    <Grid>
        <StackPanel>
            <TextBlock Text="Colaborador Responsável" Margin="10"/>
            <TextBox x:Name="txtColabResponsavel" Margin="10"/>
            <Button Content="Finalizar Registro" Margin="10" Click="Salvar_Click"/>
        </StackPanel>
    </Grid>
</Window>
